duma fibonacci
var{
 	integer a, b, auxiliar, i, n;
}
inanis initium()
{  
  a = 0;
  b = 1;
  i = 0;
 
  scriboln("Digite um numero: ");
  lectio(n);
  scriboln("Serie de Fibonacci:\n");
  scriboln(b , "\n");
 
  dum(i<n)
  {
    auxiliar = a + b;
    a = b;
    b = auxiliar;
    i = i + 1;
 
    scriboln(auxiliar , "\n");
  }
}